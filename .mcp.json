{
  "mcpServers": {
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    },
    "supabase-mcp": {
      "command": "node",
      "args": [
        "C:\\projects\\mcp-servers\\selfhosted-supabase-mcp\\dist\\index.js",
        "--url",
        "https://db.npca.com.au",
        "--anon-key",
        "${SUPABASE_ANON_KEY}",
        "--service-key",
        "${SUPABASE_SERVICE_KEY}",
        "--db-url",
        "${DATABASE_URL}"
      ],
      "alwaysAllow": [
        "list_tables",
        "get_database_connections",
        "get_database_stats",
        "get_project_url",
        "get_service_key",
        "get_anon_key",
        "execute_sql"
      ]
    }
  }
}